// socials list
.social-list
  .container.container--8
    .social-list__head.section-head(data-aos="fade-up")
      h2.h3-title Socials
    .social-list__grid
      .social-list__col(data-aos="fade-up")
        a(href="#" target="_blank").social-list__element
          .social-list__icon
            +svgMono('social-instagram')
          .social-list__name.h6-title Instagram
      .social-list__col(data-aos="fade-up")
        a(href="#" target="_blank").social-list__element
          .social-list__icon
            +svgMono('social-facebook')
          .social-list__name.h6-title Facebook
      .social-list__col(data-aos="fade-up")
        a(href="#" target="_blank").social-list__element
          .social-list__icon
            +svgMono('social-twitter')
          .social-list__name.h6-title Twitter
      .social-list__col(data-aos="fade-up")
        a(href="#" target="_blank").social-list__element
          .social-list__icon
            +svgMono('social-medium')
          .social-list__name.h6-title Medium
