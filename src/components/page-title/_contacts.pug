// PAGE TITLE
.page-title.page-title--dark
  .container
    .page-title__bg-coins
      include ../../icons/svg-inline/coins-bg-page-title.svg
    .page-title__head(data-aos="fade-up")
      h1.h1-title.h3-title-md Contacts

  .container.container--8
    // offices
    .page-title__offices.offices
      .offices__head(data-aos="fade-up")
        .h3-title.h4-title-md Offices
      .offices__wrapper
        .offices__col
          .offices__element(data-aos="fade-up")
            .offices__icon
              +svgMono('map-point')
            .offices__content
              .h6-title Minsk, Belarus
              .h6-title.c-primary Prushinsky 31A, office 109
        .offices__col
          .offices__element(data-aos="fade-up")
            .offices__icon
              +svgMono('map-point-big')
            .offices__content
              .h6-title Moscow, Russia
              .h6-title.c-primary Sadovaya B., d. 5, building. 2, office 2012
        .offices__col
          .offices__element(data-aos="fade-up")
            .offices__icon
              +svgMono('map-point')
            .offices__content
              .h6-title Hrodna, Belarus
              .h6-title.c-primary Gorky street 49, of. 619

  .page-title__waves.bg-waves.bg-waves--bottom.bg-waves--dark.bg-waves--h-small
    include ../../icons/svg-inline/waves-up.svg
