// REG CTA
.reg-cta
  .container.container--8
    .reg-cta__holder(data-aos="fade-up")
      .reg-cta__wrapper
        .reg-cta__content
          h3.h3-title=t('reg-cta.title')
          p.p-small.c-gray=t('reg-cta.description')
          a(href="#").btn.btn-primary=t('reg-cta.button')
        .reg-cta__image
          include ../../icons/svg-inline/illustration_reg-cta.svg
