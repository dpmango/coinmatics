// OUR story
.our-story
  .container.container--8
    .our-story__row
      .our-story__image(data-aos="fade")
        img(js-lazy src='img/our-story.png' data-srcset='img/our-story@2x.png 2x')
        //- include ../../icons/svg-inline/our-story.svg
      .our-story__content(data-aos="fade-up")
        .section-head.section-head--left
          h3.h3-title=t('our-story.storyTitle')
          p.p-small.p-main-md.c-gray=t('our-story.storyDsc')

    .our-story__row
      .our-story__content.our-story__content--pl40(data-aos="fade-up")
        .section-head.section-head--left
          h3.h3-title=t('our-story.missionTitle')
          p.p-small.p-main-md.c-gray=t('our-story.missionDsc')
      .our-story__image(data-aos="fade")
        img(js-lazy src='img/our-mission.png' data-srcset='img/our-mission@2x.png 2x')
        //- include ../../icons/svg-inline/our-mission.svg
