extends shared/_layout
block variables
  - var title = t('titles.faq')
  - var metaDescription = t('meta.faq.description')
  - var metaKeywords = t('meta.common.keywords')
  - var opGraphImage = 'https://coinmatics.com/img/opgraph/main.png'
  - var opGraphTitle = t('meta.faq.opgraph.title')
  - var opGraphDescription = t('meta.faq.opgraph.description')

block content

  // header modifier
  div(js-header-class data-class="dark-background")

  include ../components/page-title/_faq
  
  if locale === 'ru'
    include ../components/content/_faq-ru
  else
    include ../components/content/_faq-en

  include ../components/reg-cta/_reg-cta
